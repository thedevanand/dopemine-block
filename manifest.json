{
  "manifest_version": 2,
  "name": "Dopemine Block",
  "version": "1.0",
  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "webNavigation",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Dopemine Block",
    "default_popup": "options.html"
  }
}

